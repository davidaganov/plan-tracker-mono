{
  "name": "@plans-tracker/types",
  "version": "1.0.1",
  "description": "Shared TypeScript types for Plans Tracker",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly && tsc-alias",
    "dev": "tsup --watch",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.54.0"
  }
}
